{
  "name": "vue-image-carousel",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --no-watch",
    "test:coverage": "vitest --no-watch --coverage",
    "test": "vitest"
  },
  "pre-push": [
    "build",
    "test:unit"
  ],
  "dependencies": {
    "@faker-js/faker": "^6.1.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@vueuse/core": "^8.3.1",
    "hammer-touchemulator": "^0.0.2",
    "hammerjs": "^2.0.8",
    "scroll-into-view-if-needed": "^2.2.29",
    "smooth-scroll-into-view-if-needed": "^1.1.33",
    "vue": "^3.2.31",
    "vue-spinner": "^1.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.5.1",
    "@types/hammerjs": "^2.0.41",
    "@types/node": "^17.0.25",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.2",
    "happy-dom": "^2.55.0",
    "postcss": "^8.4.12",
    "pre-push": "^0.1.2",
    "prettier": "^2.6.1",
    "sass": "^1.32.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "url": "^0.11.0",
    "vite": "^2.9.5",
    "vitest": "^0.9.4",
    "vue-tsc": "^0.29.8"
  }
}
